    "version": "0.2.0",
    "configurations": [
        {
            "type": "msedge",
            "request": "launch",
            "name": "Open index.html",
            "file": "f:\\Portifolio\\index.html"
        },
        {
            "name": "Launch Edge against localhost",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "serve",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ]
        }
    ]
